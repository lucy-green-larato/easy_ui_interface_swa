{
  "version": "2.0",
  "functionTimeout": "00:06:00",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    },
    "durableTask": {
      "hubName": "CampaignHub"
    },
    "queues": {
      "maxDequeueCount": 5,
      "visibilityTimeout": "00:09:30",
      "batchSize": 8,
      "newBatchThreshold": 4,
      "messageEncoding": "None"
    }
  },
  "logging": {
    "logLevel": {
      "Function": "Information",
      "Host.Results": "Information"
    }
  },
  "functions": [
    "auth",
    "engagement-generate",
    "ixbrl-financials",
    "pbi_token",
    "qualification-generate",
    "ch-strategic-start",
    "ch-strategic",
    "ch-strategic-worker",
    "campaign-start",
    "campaign-worker",
    "campaign-evidence",
    "campaign-outline",
    "campaign-router",
    "campaign-markdown-pack",
    "campaign-packsload",
    "campaign-competitor-enrich",
    "campaign-competitor-score",
    "campaign-linkedin",
    "campaign-write",
    "campaign-status",
    "campaign-fetch",
    "campaign-pillars"
  ]
}