{
  "version": "2.0",
  "functionTimeout": "00:06:00",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    },
    "durableTask": {
      "hubName": "CampaignHub"
    },
    "queues": {
      "maxDequeueCount": 5,
      "visibilityTimeout": "00:00:30",
      "batchSize": 8,
      "newBatchThreshold": 4
    }
  },
  "logging": {
    "logLevel": {
      "Function": "Information",
      "Host.Results": "Information"
    }
  },
   "functions": [
    "auth",
    "engagement-generate",
    "ixbrl-financials",
    "pbi_token",
    "qualification-generate",
    "ch-strategic-start",
    "ch-strategic",
    "ch-strategic-worker",
    "campaign-fetch",
    "campaign-start",
    "campaign-status",
    "campaign-worker"
  ]
}