{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/content/*",
      "/src/*",
      "/vendor/*",
      "/assets/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "https://fa-sales-accelerator.azurewebsites.net/api/{*path}"
    },
    { "route": "/call-script/*", "rewrite": "/call-script/index.html" },
    { "route": "/qualification/*", "rewrite": "/qualification/index.html" },
    { "route": "/engagement/*", "rewrite": "/engagement/index.html" }
  ],
  "mimeTypes": {
    ".md": "text/markdown",
    ".json": "application/json"
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html" }
  }
}
