{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/.auth/*",
      "/api/*",
      "/assets/*",
      "/vendor/*",
      "/content/*",
      "/src/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/engagement/*",
      "rewrite": "/engagement/index.html"
    },
    {
      "route": "/qualification/*",
      "rewrite": "/qualification/index.html"
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".md": "text/markdown"
    }
}
